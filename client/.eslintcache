-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/;\n    isValid = pattern.test(value) && isValid;\n  }\n\n  if (rules.isNumeric) {\n    const pattern = /^\\d+$/;\n    isValid = pattern.test(value) && isValid;\n  }\n\n  if(rules.isUrl) {\n    const pattern = /^(http:\\/\\/www\\.|https:\\/\\/www\\.|http:\\/\\/|https:\\/\\/)?[a-z0-9]+([\\-\\.]{1}[a-z0-9]+)*\\.[a-z]{2,5}(:[0-9]{1,5})?(\\/.*)?$/g;\n    // const regex = new RegExp(pattern);\n    isValid = pattern.test(value) && isValid\n  }\n\n  return isValid;\n};\n\nexport const linkValidator = (url) => {\n  const expression =\n    \"^(http[s]?:\\\\/\\\\/(www\\\\.)?|ftp:\\\\/\\\\/(www\\\\.)?|www\\\\.){1}([0-9A-Za-z-\\\\.@:%_~#=]+)+((\\\\.[a-zA-Z]{2,3})+)(/(.)*)?(\\\\?(.)*)?\";\n\n  const regex = new RegExp(expression);\n\n  if (url.match(regex)) {\n    return true;\n  } else {\n    return false;\n  }\n};\n\nexport const previewLink = (item) => {\n  // check if gift link is valid\n  if (linkValidator(item.url) === false) {\n    return null;\n  } else {\n    return (\n     \n        <Microlink\n          size=\"large\"\n          style={{position:'relative' , border: 'none', zIndex: '0', top: '50%', left: '50%',   transform: 'translate(-50%, -50%)', height: '80%', width: '90%'}}\n          url={item.url}\n          setData={(data) => ({\n            ...data,\n            description: null,\n          })}\n        />\n    );\n  }\n};\n","/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/store/actions/actionTypes.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/AddItem/AddItem.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/hooks/useCustomForm.js",["261","262"],"import { useState, useEffect, useRef } from \"react\";\n\nconst useCustomForm = ({\n  initialValues,\n  onSubmit\n}) => {\n  const [values, setValues] = useState(initialValues || {});\n  const [errors, setErrors] = useState({});\n  const [touched, setTouched] = useState({});\n  const [onSubmitting, setOnSubmitting] = useState(false);\n  const [onBlur, setOnBlur] = useState(false);\n\n  const formRendered = useRef(true);\n\n  useEffect(() => {\n    if (formRendered.current) {\n      setValues(initialValues);\n      setErrors({});\n      setTouched({});\n      setOnSubmitting(false);\n      setOnBlur(false);\n    }\n    formRendered.current = false;\n  }, [initialValues]);\n\n  const handleChange = (event) => {\n    const { target } = event;\n    const { name, value } = target;\n    event.persist();\n    setValues({ ...values, [name]: value });\n};\n\n  const handleBlur = (event) => {\n    const { target } = event;\n    const { name } = target;\n    setTouched({ ...touched, [name]: true });\n    setErrors({ ...errors });\n};\n\n  const handleSubmit = (event) => {\n    if (event) event.preventDefault();\n    setErrors({ ...errors });\n    onSubmit({ event });\n  };\n\n  return {\n    values,\n    errors,\n    touched,\n    handleChange,\n    handleBlur,\n    handleSubmit\n  };\n};\n\nexport default useCustomForm;",["263","264"],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/containers/Auth/Logout/Logout.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/store/actions/index.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/containers/Discover/Discover.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemsList.js",["265"],"import React, { useEffect } from \"react\";\nimport { connect } from \"react-redux\";\nimport { fetchLoggedUserItems, fetchAllItems, fetchItemsByUserId} from '../../../store/actions/items'\nimport axios from \"axios\";\nimport withErrorHandler from \"../../../hoc/withErrorHandler/withErrorHandler\";\nimport Spinner from \"../../UI/Spinner/Spinner\";\nimport ItemCard from \"./ItemCard/ItemCard\";\n\nimport classes from \"./ItemsList.module.css\";\n\nconst ItemsList = ({ fetchAllItems, fetchLoggedUserItems, fetchItemsByUserId , items, loading, cardType, userId}) => {\n  useEffect(() => {\n    if(cardType === 'profile')\n  return fetchLoggedUserItems();\n    if(cardType === 'discover')\n    return fetchAllItems();\n    if(cardType ==='friend'){\n      return fetchItemsByUserId(userId)\n    }\n\n  }, [ fetchLoggedUserItems, fetchAllItems, fetchItemsByUserId ]);\n\n  let fetchedItems = (\n    <div className={classes.Spinner}>\n      <Spinner />\n    </div>\n  );\n  if (!loading && items !== null) {\n    fetchedItems = items.map((item => (<ItemCard key={item.id} item={item} cardType={cardType}/>\n      )))\n  } else if (!loading && items === null) {\n    fetchedItems = (\n      <div className={classes.NoItemsContainer}>\n        <h2>\n          No items here..<br></br>\n          <br></br>Add More Items to wishlist\n        </h2>\n      </div>\n    );\n  }\n  return <div className={classes.ItemsContainer}>{fetchedItems}</div>;\n};\n\nconst mapStateToProps = (state) => {\n  return {\n    items: state.items.userItems,\n    loading: state.items.loading,\n\n  };\n};\n\nexport default connect(\n  mapStateToProps,\n  { fetchAllItems, fetchLoggedUserItems, fetchItemsByUserId }\n)(withErrorHandler(ItemsList, axios));\n","/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/hoc/withErrorHandler/withErrorHandler.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/hooks/http-error-handler.js",["266","267"],"import {useState, useEffect} from 'react';\n\nexport default httpClient => {\n    const [error, setError] = useState(null);\n\n    const reqInterceptor = httpClient.interceptors.request.use((req) => {\n      setError(null);\n      return req;\n    });\n    const resInterceptor = httpClient.interceptors.response.use(\n      (res) => res,\n      (err) => {\n        setError(err);\n      }\n    );\n\n    useEffect(() => {\n      return () => {\n        httpClient.interceptors.request.eject(reqInterceptor);\n        httpClient.interceptors.response.eject(resInterceptor);\n      };\n    }, [reqInterceptor, resInterceptor]);\n\n    const errorConfirmedHandler = () => {\n      setError(null);\n    };\n\n    return [error, errorConfirmedHandler];\n}","/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/ItemCard.js",["268"],"import React, { useState, useEffect } from \"react\";\nimport { previewLink } from \"../../../../shared/utility\";\nimport BottomBar from \"./BottomBar/BottomBar\";\nimport moment from \"moment\";\nimport Button from \"../../../UI/Button/Button\";\n\nimport classes from \"./ItemCard.module.css\";\nimport ItemSettings from \"./ItemSettings/ItemSettings\";\nimport { Link } from \"react-router-dom\";\n\nconst ItemCard = ({ item, cardType}) => {\n  const [isSettingsOpen, updateSettings] = useState(false);\n\n\nuseEffect(() => {\n  previewLink(item);\n}, [previewLink])\n\n  const handleItemSettings = () => {\n    if (!isSettingsOpen) {\n      updateSettings(true);\n    } else {\n      updateSettings(false);\n    }\n  };\n\n  return item ? (\n    <div key={item.id} className={classes.CardContainer}>\n      <div>\n        <Link to={`/user/${item._user._id}`}>\n        <img src={item._user.avatar} alt=\"\" className={classes.userAvatar}/>\n        <p className={classes.userName}>{item._user.firstName} {item._user.lastName}</p>\n        </Link>\n        <span className={classes.Date}>\n          {moment(item.dateCreated).format(\"MMM Do YYYY\")}\n        </span>\n        {cardType === \"discover\" ? null : (\n          <Button clicked={() => handleItemSettings()} btnType=\"ItemSettings\">\n            <i className={[classes.SettingsIcon, \"material-icons\"].join(\" \")}>\n              more_horiz\n            </i>\n          </Button>\n        )}\n      </div>\n\n      <ItemSettings item={item} isSettingsOpen={isSettingsOpen} />\n      <div className={classes.Preview}>{previewLink(item)}</div>\n      <BottomBar item={item} cardType={cardType}/>\n    </div>\n  ) : null;\n};\n\n\nexport default (ItemCard);\n","/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/BottomBar/BottomBar.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/BottomBar/BarItems/Like/Like.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/BottomBar/BarItems/Save/Save.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/BottomBar/BarItems/Share/Share.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/BottomBar/BarItems/Share/ShareForm/ShareForm.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/UI/Input/Input.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/ItemSettings/ItemSettings.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/store/reducers/auth.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/store/reducers/items.js",[],"/media/mark/71290e93-e48d-4f57-98d4-31607986