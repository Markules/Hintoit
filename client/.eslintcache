div>\n\n        <div className={classes.loginContainer}>\n          <div className={classes.loginContent}>\n            <button className={classes.googleLoginButton}>\n              <a className={classes.buttonContent} href=\"/auth/google\">\n                Sign In with Google\n              </a>\n            </button>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nfunction mapStateToProps(state) {\n  return {\n    isAuthenticated: state.auth.userId !== null\n  };\n}\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    login: () => dispatch(actions.auth()),\n  };\n};\nexport default connect(mapStateToProps, mapDispatchToProps)(Login);\n","/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/containers/Auth/store/actions/auth.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/UI/Spinner/Spinner.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/containers/Auth/store/reducers/auth.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/hoc/Aux/Aux.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/UI/Button/Button.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/UI/Modal/Modal.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Navigation/ToolBar/ToolBar.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/UI/Backdrop/Backdrop.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Logo/Logo.js",[],"/media/mark/71290e93