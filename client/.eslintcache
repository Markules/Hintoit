n\";\n\nimport classes from \"./FollowButton.module.css\";\n\nconst FollowButton = ({ followUser, unFollowUser, user, loggedUser }) => {\n  const [isFollow, setFollowStatus] = useState(\n    loggedUser.following.includes(user._id)\n  );\n\n  const [button, setButton ] = useState(null)    \n\n  useEffect(() => {\n    isFollow\n      ? (setButton(\n          <Button\n            className={classes.ButtonContainer}\n            clicked={() => onUnFollow(user._id)}\n            btnType={\"Follow\"}\n          >\n            Unfollow\n          </Button>\n        ))\n      : (setButton(\n          <Button\n            className={classes.ButtonContainer}\n            clicked={() => onFollow(user._id)}\n            btnType={\"Follow\"}\n          >\n            Follow\n          </Button>\n        ));\n  }, []);\n\n  const onFollow = (id) => {\n    followUser(id);\n    setFollowStatus(true);\n    setButton(<Button\n        className={classes.ButtonContainer}\n        clicked={() => onUnFollow(user._id)}\n        btnType={\"Follow\"}>Unfollow</Button>)\n  }\n\n  const onUnFollow = (id) => {\n    unFollowUser(id);\n    setFollowStatus(false);\n    setButton(  <Button\n        className={classes.ButtonContainer}\n        clicked={() => onFollow(user._id)}\n        btnType={\"Follow\"}\n      >Follow</Button>)\n  }\n\n  return <Fragment>{button}</Fragment>;\n};\n\nFollowButton.propTypes = {\n  followUser: PropTypes.func.isRequired,\n  unFollowUser: PropTypes.func.isRequired,\n  loggedUser: PropTypes.object.isRequired,\n  user: PropTypes.object.isRequired,\n};\n\nconst mapStateToProps = (state) => ({\n  loggedUser: state.profile.userData,\n});\n\nexport default connect(mapStateToProps, { followUser, unFollowUser })(\n  FollowButton\n);\n","/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/hoc/Layout/NotFound/NotFound.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/