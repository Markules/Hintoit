isValid = value.length <= rules.maxLength && isValid;\n  }\n\n  if (rules.isEmail) {\n    const pattern = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/;\n    isValid = pattern.test(value) && isValid;\n  }\n\n  if (rules.isNumeric) {\n    const pattern = /^\\d+$/;\n    isValid = pattern.test(value) && isValid;\n  }\n\n  if(rules.isUrl) {\n    const pattern = /^(http:\\/\\/www\\.|https:\\/\\/www\\.|http:\\/\\/|https:\\/\\/)?[a-z0-9]+([\\-\\.]{1}[a-z0-9]+)*\\.[a-z]{2,5}(:[0-9]{1,5})?(\\/.*)?$/g;\n    // const regex = new RegExp(pattern);\n    isValid = pattern.test(value) && isValid\n  }\n\n  return isValid;\n};\n\nexport const linkValidator = (url) => {\n  const expression =\n    \"^(http[s]?:\\\\/\\\\/(www\\\\.)?|ftp:\\\\/\\\\/(www\\\\.)?|www\\\\.){1}([0-9A-Za-z-\\\\.@:%_~#=]+)+((\\\\.[a-zA-Z]{2,3})+)(/(.)*)?(\\\\?(.)*)?\";\n\n  const regex = new RegExp(expression);\n\n  if (url.match(regex)) {\n    return true;\n  } else {\n    return false;\n  }\n};\n\nexport const previewLink = (item) => {\n  // check if gift link is valid\n  if (linkValidator(item.url) === false) {\n    return null;\n  } else {\n    return (\n     \n        <Microlink\n          size=\"large\"\n          style={{position:'relative' , border: 'none', zIndex: '0', top: '50%', left: '50%',   transform: 'translate(-50%, -50%)', height: '80%', width: '90%'}}\n          url={item.url}\n          setData={(data) => ({\n            ...data,\n            description: null,\n          })}\n        />\n    );\n  }\n};\n","/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/store/actions/actionTypes.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/AddItem/AddItem.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/hooks/useCustomForm.js",["268","269"],"import { useState, useEffect, useRef } from \"react\";\n\nconst useCustomForm = ({\n  initialValues,\n  onSubmit\n}) => {\n  const [values, setValues] = useState(initialValues || {});\n  const [errors, setErrors] = useState({});\n  const [touched, setTouched] = useState({});\n  const [onSubmitting, setOnSubmitting] = useState(false);\n  const [onBlur, setOnBlur] = useState(false);\n\n  const formRendered = useRef(true);\n\n  useEffect(() => {\n    if (formRendered.current) {\n      setValues(initialValues);\n      setErrors({});\n      setTouched({});\n      setOnSubmitting(false);\n      setOnBlur(false);\n    }\n    formRendered.current = false;\n  }, [initialValues]);\n\n  const handleChange = (event) => {\n    const { target } = event;\n    const { name, value } = target;\n    event.persist();\n    setValues({ ...values, [name]: value });\n};\n\n  const handleBlur = (event) => {\n    const { target } = event;\n    const { name } = target;\n    setTouched({ ...touched, [name]: true });\n    setErrors({ ...errors });\n};\n\n  const handleSubmit = (event) => {\n    if (event) event.preventDefault();\n    setErrors({ ...errors });\n    onSubmit({ event });\n  };\n\n  return {\n    values,\n    errors,\n    touched,\n    handleChange,\n    handleBlur,\n    handleSubmit\n  };\n};\n\nexport default useCustomForm;",["270","271"],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/containers/Auth/Logout/Logout.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/store/actions/index.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/containers/Discover/Discover.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemsList.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/hoc/withErrorHandler/withErrorHandler.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/hooks/http-error-handler.js",["272"],"import {useState, useEffect} from 'react';\n\n  export default httpClient  => {\n    const [error, setError] = useState(null);\n\n    const reqInterceptor = httpClient.interceptors.request.use((req) => {\n      setError(null);\n      return req;\n    });\n    const resInterceptor = httpClient.interceptors.response.use(\n      (res) => res,\n      (err) => {\n        setError(err);\n      }\n    );\n\n    useEffect(() => {\n      return () => {\n        httpClient.interceptors.request.eject(reqInterceptor);\n        httpClient.interceptors.response.eject(resInterceptor);\n      };\n    }, [reqInterceptor, resInterceptor, httpClient.interceptors.request, httpClient.interceptors.response ]);\n\n    const errorConfirmedHandler = () => {\n      setError(null);\n    };\n\n    return [error, errorConfirmedHandler];\n}","/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/ItemCard.js",["273"],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/BottomBar/BottomBar.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/BottomBar/BarItems/Like/Like.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/BottomBar/BarItems/Save/Save.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersion/client/src/components/Items/ItemsList/ItemCard/BottomBar/BarItems/Share/Share.js",[],"/media/mark/71290e93-e48d-4f57-98d4-3160798637ce/home/mark/progremming/nodejs/hintoit-newVersi